/* 引入字体图标 */
@import './styles/iconfont.wxss';

/* 在微信小程序中是不支持通配符的 */
/* 初始化 */
page,view,text,swiper,swiper-item,image,navigator{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

/* 
    主题颜色 通过变量实现
    在less中是支持变量存在的
    原生的css和wxss也是支持 变量的
*/

page{
    /* 定义主题颜色 */
    --themeColor:#eb4450;
    /* 定义统一的字体大小 */
    font-size: 28rpx;
}


image{
    width: 100%;
}